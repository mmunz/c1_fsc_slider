<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <langChildren>1</langChildren>
    </meta>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.header_tag>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:header_tag
                            </label>
                            <config>
                                <type>select</type>
<renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">span</numIndex>
                                        <numIndex index="1">span</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">h1</numIndex>
                                        <numIndex index="1">h1</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">h2</numIndex>
                                        <numIndex index="1">h2</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">h3</numIndex>
                                        <numIndex index="1">h3</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">h4</numIndex>
                                        <numIndex index="1">h4</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.header_tag>
                </el>
            </ROOT>
        </general>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Slider Options
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <!-- Checkbox -->
                    <options.autoplay>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.autoplay
                            </label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </options.autoplay>
                    <options.infinite>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.infinite
                            </label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </options.infinite>
                    <options.fade>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.fade
                            </label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </options.fade>
                    <options.waitForAnimate>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.waitForAnimate
                            </label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </options.waitForAnimate>
                    <options.dots>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.dots
                            </label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </options.dots>
                    <options.arrows>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.arrows
                            </label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </options.arrows>
                    <options.speed>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.speed
                            </label>
                            <config>
                                <type>input</type>
                                <default>2000</default>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </options.speed>
                    <options.autoplaySpeed>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>
                                LLL:EXT:c1_fsc_slider/Resources/Private/Language/locallang_be.xlf:flexform.autoplaySpeed
                            </label>
                            <config>
                                <type>input</type>
                                <default>4000</default>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </options.autoplaySpeed>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
